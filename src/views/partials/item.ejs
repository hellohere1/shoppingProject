<div class="itemDiv">
    <p>Item Name: <%= item.itemName  %></p>
    <p>Quantity:<%= item.quantity  %></p>
    <input type="checkbox" class="checkedbox"  value=<%=item.itemName%> <%= item.isBought ? "checked" : ''%> />
</div>
<div style="width: 70%; float: right;">
    <button value=<%=item._id%> onclick="deleteTheItem(this.value)" >delete item <%= item.itemName %></button>
</div>
